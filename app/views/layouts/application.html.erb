<!DOCTYPE html>
<html>
  <head>
    <title>FinalEthanHunter934</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <h1>Taylor Swift: Evermore </h1>
    <table>
      <tr>
        <th>Loves</th>
        <th>Likes</th>
        <th>Song Name</th>
      </tr>
      <% @songs.each do |song| %>  
        <tr>
          <td> <%="#{song.loves"} %> </td>
          <td> <%="#{song.likes"} %> </td>
          <td> <%="#{song.name"} %> </td>
        </tr>
      <% end %>
    </table>
  </body>
</html>
